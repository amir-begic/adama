model Standings {
  id            Int      @id @default(autoincrement())
  teamId        Int
  user          User     @relation(fields: [userId], references: [id])    
  userId        Int      @unique  
  totalPlayed   Int
  wins          Int
  draws         Int
  losses        Int
  points        Int
  goalsFor      Int
  goalsAgainst  Int
  goalDifference Int
  createdAt     DateTime @default(now())
}

